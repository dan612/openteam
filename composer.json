{
    "name": "drupal/openteam",
    "description": "Openteam Distribution",
    "type": "project",
    "authors": [
        {
            "name": "Dan Chadwick",
            "email": "daniel.chadwick612@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.0.0"
    },
    "extra": {
      "installer-types": [
        "bower-asset",
        "npm-asset"
      ],
      "installer-paths": {
        "docroot/core": [
          "type:drupal-core"
        ],
        "docroot/modules/custom/{$name}": [
          "type:custom"
        ],
        "docroot/libraries/{$name}": [
          "type:drupal-library",
          "type:bower-asset",
          "type:npm-asset"
        ],
        "docroot/modules/contrib/{$name}": [
          "type:drupal-module"
        ],
        "docroot/profiles/contrib/{$name}": [
          "type:drupal-profile"
        ],
        "docroot/profiles/custom/{$name}": [
          "type:profile"
        ],
        "docroot/themes/contrib/{$name}": [
          "type:drupal-theme"
        ],
        "drush/contrib/{$name}": [
          "type:drupal-drush"
        ]
      },
      "enable-patching": true
    }
}
